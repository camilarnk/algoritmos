Algoritmo "13)lista1"

Var
    num1, num2, num3, result: inteiro

Inicio
    escreva("Primeiro número: ")
    leia(num1)
    escreva("Segundo número: ")
    leia(num2)
    escreva("Terceiro número: ")
    leia(num3)
    result <- (num1+num3)*num2
    escreva("(",num1," +",num3," ) *",num2," =",result)

Fimalgoritmo