Algoritmo "9_lista1_volprism"

Var
    l1, l2, l3, vpm: real

Inicio
    escreva("Lado 1 do prisma: ")
    leia(l1)
    escreva("Lado 2 do prisma: ")
    leia(l2)
    escreva("Lado 3 do prisma: ")
    leia(l3)
    vpm <- l1 * l2 * l3
    escreva("Volume do prisma:",vpm)


Fimalgoritmo