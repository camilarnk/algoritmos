Algoritmo "12)lista1"

Var
    num1, num2, num3, mult: inteiro

Inicio
    escreva("Número 1: ")
    leia(num1)
    escreva("Número 2: ")
    leia(num2)
    escreva("Número 3: ")
    leia(num3)
    mult <- num1*num2*num3
    escreva("A multiplicação entre",num1,",",num2," e",num3," é igual a:",mult)
    
Fimalgoritmo