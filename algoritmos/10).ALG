Algoritmo "10)"

Var
    a, c, l, z: real

Inicio
   escreva("Digite o primeiro número: ")
   leia(l)
   escreva("Digite o segundo número: ")
   leia(a)
   escreva("Digite o terceiro número: ")
   leia(c)
   z <- (l+a*2)/c
   escreva("O resultado de (",l," +",a," * 2 ) /",c," é igual a",z,"")



Fimalgoritmo