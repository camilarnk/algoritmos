Algoritmo "semnome"

Var
    nome: caracter
    idade, cont: inteiro

Inicio
    cont <- 0
    para cont de 0 ate 2 faca
        cont <- cont+1
        escreva("Digite seu nome: ")
        leia(nome)
        escreva("Digite sua idade: ")
        leia(idade)
        escreval("Nome: ",nome," Idade:",idade)
    fimpara


Fimalgoritmo