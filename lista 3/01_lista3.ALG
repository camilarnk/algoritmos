Algoritmo "semnome"

Var
    nome: caracter
    idade, cont: inteiro

Inicio
    cont <- 0
    repita
        cont <- cont+1
        escreva("Digite seu nome: ")
        leia(nome)
        escreva("Digite sua idade: ")
        leia(idade)
        escreval("Nome: ",nome," Idade:",idade)
    ate (cont=3)


Fimalgoritmo